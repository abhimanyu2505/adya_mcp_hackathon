# TeamSpeak MCP Configuration for Docker
# Copy this file to .env or configure variables in docker-compose.yml

# TeamSpeak server address
TEAMSPEAK_HOST=my-server.teamspeak.com

# ServerQuery port (default: 10011)
TEAMSPEAK_PORT=10011

# ServerQuery username
TEAMSPEAK_USER=mcp_user

# ServerQuery password
TEAMSPEAK_PASSWORD=my_secure_password

# Virtual server ID (default: 1)
TEAMSPEAK_SERVER_ID=1

# Additional configuration for Docker
# These variables can be used for debugging
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1 